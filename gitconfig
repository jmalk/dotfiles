# This is Git's per-user configuration file.
[user]
  name = Josh Malkinson
  email = josh.malkinson@lush.co.uk
[alias]
  a = add
  bi = bisect
  br = branch
  ci = commit
  d = diff
  ds = diff --staged
  l = log
  m = merge
  pl = pull
  ps = push
  r = restore
  s = status
  sw = switch
[pull]
  rebase = true
[fetch]
  prune = true
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[init]
  defaultBranch = main
[push]
  autoSetupRemote = true
