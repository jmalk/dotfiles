# This is Git's per-user configuration file.
[user]
  name = Josh Malkinson
  email = joshua.malkinson@coop.co.uk
[alias]
  a = add
  bi = bisect
  br = branch
  ci = commit
  co = checkout
  d = diff
  ds = diff --staged
  l = log
  la = last
  last = log -1 HEAD -p
  lo = log --oneline
  m = merge
  p = pull
  ps = push
  r = restore
  rb = rebase
  rs = restore --staged
  s = status
  st = stash
  sw = switch
  unst = unstage
  unstage = restore --staged --
[pull]
  rebase = true
[fetch]
  prune = true
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[init]
  defaultBranch = main
[push]
  autoSetupRemote = true
